{
  "manifest_version": 3,
  "name": "SuperMind URL Sender",
  "description": "Send current tab URL to SuperMind backend for processing",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
